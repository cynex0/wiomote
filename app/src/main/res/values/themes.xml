<resources>

    <style name="Base.Theme.WIOmote" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorError">#ba1b1b</item>
        <item name="colorOnError">#ffffff</item>
        <item name="colorErrorContainer">#ffdad4</item>
        <item name="colorOnErrorContainer">#410001</item>
        <item name="backgroundColor">#FDFDF6</item>
        <item name="colorPrimaryContainer">#F4F6EA</item>
        <item name="colorSecondary">#9CD67D</item>
        <item name="colorOnSecondary">#205107</item>
        <item name="colorSecondaryContainer">#F4F6EA</item>
        <item name="colorOnSecondaryContainer">#66205107</item>

        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="Theme.WIOmote" parent="Base.Theme.WIOmote">
        <item name="android:navigationBarColor">?attr/backgroundColor</item>
        <item name="android:navigationBarDividerColor">?attr/backgroundColor</item>
        <item name="android:statusBarColor">?attr/backgroundColor</item>
        <item name="android:windowBackground">?attr/backgroundColor</item>
    </style>
</resources>